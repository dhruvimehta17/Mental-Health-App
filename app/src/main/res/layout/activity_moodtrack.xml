<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B0CA9F"
    android:backgroundTint="#B0CA9F"
    tools:context=".moodtrack">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="310dp"
        android:layout_height="42dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/corner"
        android:gravity="top"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:text="How are you feelings right now?"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/happy1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints,RtlCompat" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="281dp"
        android:layout_height="47dp"
        android:layout_marginTop="4dp"
        android:fontFamily="serif"
        android:gravity="bottom"
        android:text="Mood Tracking"
        android:textAlignment="center"
        android:textColor="#435339"
        android:textSize="35dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="RtlCompat" />

    <ImageButton
        android:id="@+id/sad1"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginBottom="8dp"
        android:background="#B0CA9F"
        android:contentDescription="Sad Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_5"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.237"
        app:layout_constraintStart_toEndOf="@+id/med1" />

    <ImageButton
        android:id="@+id/happy1"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginBottom="8dp"
        android:background="#B0CA9F"
        android:contentDescription="Happy Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_3"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/med1"
        app:layout_constraintHorizontal_bias="0.73"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/med1"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="#B0CA9F"
        android:contentDescription="Neutral Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_4"
        app:layout_constraintEnd_toStartOf="@+id/sad1"
        app:layout_constraintStart_toEndOf="@+id/happy1"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="310dp"
        android:layout_height="42dp"
        android:background="@drawable/corner"
        android:gravity="top"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:text="How did your day start?"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.236"
        tools:ignore="MissingConstraints,RtlCompat" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="310dp"
        android:layout_height="59dp"
        android:background="@drawable/corner"
        android:gravity="top"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:text="How do you feel after interacting with others today?"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.398"
        tools:ignore="MissingConstraints,RtlCompat" />

    <ImageButton
        android:id="@+id/sad3"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:background="#B0CA9F"
        android:contentDescription="Sad Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_5"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.237"
        app:layout_constraintStart_toEndOf="@+id/med3"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.342" />

    <ImageButton
        android:id="@+id/happy3"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:background="#B0CA9F"
        android:contentDescription="Happy Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_3"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/med3"
        app:layout_constraintHorizontal_bias="0.73"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.342" />

    <ImageButton
        android:id="@+id/med3"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:background="#B0CA9F"
        android:contentDescription="Neutral Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_4"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/sad3"
        app:layout_constraintStart_toEndOf="@+id/happy3"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.342" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="310dp"
        android:layout_height="59dp"
        android:background="@drawable/corner"
        android:gravity="top"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:text="How do you feel about the tasks you accomplished today?"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.778"
        tools:ignore="MissingConstraints,RtlCompat" />

    <ImageButton
        android:id="@+id/sad5"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:background="#B0CA9F"
        android:contentDescription="Sad Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_5"
        app:layout_constraintBottom_toTopOf="@+id/login"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.237"
        app:layout_constraintStart_toEndOf="@+id/med5"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.238" />

    <ImageButton
        android:id="@+id/happy5"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:background="#B0CA9F"
        android:contentDescription="Happy Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_3"
        app:layout_constraintBottom_toTopOf="@+id/login"
        app:layout_constraintEnd_toStartOf="@+id/med5"
        app:layout_constraintHorizontal_bias="0.73"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.238" />

    <ImageButton
        android:id="@+id/med5"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:background="#B0CA9F"
        android:contentDescription="Neutral Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_4"
        app:layout_constraintBottom_toTopOf="@+id/login"
        app:layout_constraintEnd_toStartOf="@+id/sad5"
        app:layout_constraintStart_toEndOf="@+id/happy5"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.238" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="310dp"
        android:layout_height="59dp"
        android:background="@drawable/corner"
        android:gravity="top"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:text="How do you feel about your current energy level?"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.592"
        tools:ignore="MissingConstraints,RtlCompat" />

    <ImageButton
        android:id="@+id/med4"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="#B0CA9F"
        android:contentDescription="Neutral Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_4"
        app:layout_constraintEnd_toStartOf="@+id/sad4"
        app:layout_constraintStart_toEndOf="@+id/happy4"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageButton
        android:id="@+id/sad4"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="#B0CA9F"
        android:contentDescription="Sad Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.237"
        app:layout_constraintStart_toEndOf="@+id/med4"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageButton
        android:id="@+id/happy4"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="#B0CA9F"
        android:contentDescription="Happy Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_3"
        app:layout_constraintEnd_toStartOf="@+id/med4"
        app:layout_constraintHorizontal_bias="0.73"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageButton
        android:id="@+id/med2"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="468dp"
        android:background="#B0CA9F"
        android:contentDescription="Neutral Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/sad2"
        app:layout_constraintStart_toEndOf="@+id/happy2"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/happy2"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="#B0CA9F"
        android:contentDescription="Happy Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_3"
        app:layout_constraintEnd_toStartOf="@+id/med2"
        app:layout_constraintHorizontal_bias="0.73"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageButton
        android:id="@+id/sad2"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="#B0CA9F"
        android:contentDescription="Sad Mood Option"
        android:scaleType="fitCenter"
        android:src="@drawable/img_5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.237"
        app:layout_constraintStart_toEndOf="@+id/med2"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/login"
        android:layout_width="182dp"
        android:layout_height="48dp"
        android:backgroundTint="#E9F1EA"
        android:text="Submit"
        android:textColor="#435339"
        android:textColorHint="#9C27B0"
        android:textSize="18sp"
        android:textStyle="bold"
        app:cornerRadius="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.951"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>